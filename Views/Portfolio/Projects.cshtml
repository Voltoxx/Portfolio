@model IEnumerable<Portfolio.Models.Projets>

@{
    ViewData["Title"] = "Projects";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>All my projects</h1>
<table class="table">
	<tbody>
		@foreach (var item in Model) {
				
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.Title)
					
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Description)
				</td>
				<td>
					@*@Html.DisplayFor(modelItem => item.ImagePrincipale)*@
					<img alt="Image de @item.Title" src="@item.ImagePrincipale"/>
				</td>
			</tr>
		}
	</tbody>
</table>


